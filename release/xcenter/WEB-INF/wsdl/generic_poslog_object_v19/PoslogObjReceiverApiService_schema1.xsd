                <xs:attribute name="ForeignAmount" type="xs:decimal"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="StoreStructure">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute name="Level" type="xs:string" use="required"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
