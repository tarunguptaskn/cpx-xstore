            <xs:element ref="dtv:AuthorizationCode" minOccurs="0"/>
            <xs:element name="SerialNumber" type="xs:string" minOccurs="0"/>
            <xs:element name="GiftReceiptFlag" type="xs:boolean" minOccurs="0"/>
