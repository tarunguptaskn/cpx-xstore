   <xs:element name="POSLog" type="POSLogType" />
  
  <xs:complexType name="POSLogType">
    <xs:sequence>
      <xs:element name="Transaction" type="TransactionType" minOccurs="1" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
-->
    <xs:complexType name="TransactionType">
        <xs:sequence>
            <xs:element ref="dtv:OrganizationID"/>
            <xs:element name="RetailStoreID" type="xs:string"/>
            <xs:element name="WorkstationID" type="xs:string"/>
            <xs:element name="TillID" type="xs:string" minOccurs="0"/>
            <xs:element ref="dtv:CashDrawerID" minOccurs="0"/>
            <xs:element name="SequenceNumber" type="xs:string"/>
            <xs:element name="BusinessDayDate" type="xs:date"/>
            <xs:element name="BeginDateTime" type="xs:dateTime"/>
            <xs:element name="EndDateTime" type="xs:dateTime" minOccurs="0"/>
            <xs:element name="OperatorID" type="xs:string" minOccurs="0"/>
            <xs:element name="CurrencyCode" type="xs:string"/>
            <xs:element ref="dtv:FiscalNumber" minOccurs="0"/>
            <xs:element ref="dtv:DeviceId" minOccurs="0"/>
            <xs:element ref="dtv:FiscalSessionNumber" minOccurs="0"/>
            <xs:choice>
                <xs:element ref="dtv:ForeignCurrencyExchangeRate"/>
                <xs:element name="TenderControlTransaction" type="tns:TenderControlTransactionType"/>
                <xs:element name="TillControlTransaction" type="tns:TillControlTransactionType"/>
                <xs:element name="TenderExchange" type="tns:TenderExchangeType"/>
                <xs:element name="ControlTransaction" type="tns:ControlTransactionType"/>
                <xs:element name="EscrowTransaction" type="tns:EscrowTransactionType"/>
                <xs:element ref="dtv:TimeClockTransaction"/>
                <xs:element ref="dtv:TrainingModeTransaction"/>
                <xs:element name="POSLogControlTransaction" type="tns:POSLogControlTransactionType"/>
                <xs:element name="InventoryTransaction" type="tns:InventoryTransactionType" minOccurs="0" maxOccurs="unbounded"/>
            </xs:choice>
            <xs:element ref="dtv:PosTransactionProperties" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="RetailTransaction" type="tns:RetailTransactionType" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="CancelFlag" type="xs:boolean"/>
        <xs:attribute name="TrainingModeFlag" type="xs:boolean"/>
        <xs:attribute name="OfflineFlag" type="xs:boolean"/>
        <xs:attribute ref="dtv:TransactionType"/>
        <xs:attribute ref="dtv:AppVersion"/>
        <xs:attribute ref="dtv:crossChannelReturn"/>
        <xs:attribute ref="dtv:InventoryDocumentSubType"/>
        <xs:attribute ref="dtv:InventoryDocumentType"/>
    </xs:complexType>
    <xs:complexType name="RetailTransactionType">
        <xs:sequence>
            <xs:element name="LineItem" type="tns:LineItemType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="Total" type="tns:TotalType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="RoundedTotal" type="xs:decimal" minOccurs="0"/>
            <xs:element name="CustomerAccount" type="tns:CustomerAccountType" minOccurs="0"/>
            <xs:element name="Customer" type="tns:PoslogCustomerType" minOccurs="0"/>
            <xs:element ref="dtv:EmployeeSaleId" minOccurs="0"/>
            <xs:element name="TransactionLink" type="tns:TransactionLinkType" minOccurs="0"/>
            <xs:choice minOccurs="0">
                <xs:element ref="dtv:PostVoidReasonCode" minOccurs="0"/>
                <xs:element ref="dtv:TransCancelReasonCode" minOccurs="0"/>
            </xs:choice>
            <xs:element ref="dtv:PostVoidComment" minOccurs="0"/>
            <xs:element ref="dtv:FlightInformation" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="Version" type="xs:string"/>
        <xs:attribute name="TypeCode" type="xs:string"/>
        <xs:attribute name="TransactionStatus" type="xs:string"/>
    </xs:complexType>
    <xs:complexType name="LineItemType">
        <xs:sequence>
            <xs:element name="SequenceNumber" type="xs:string" minOccurs="0"/>
            <xs:element name="BeginDateTime" type="xs:dateTime" minOccurs="0"/>
            <xs:element name="EndDateTime" type="xs:dateTime" minOccurs="0"/>
