    <xs:complexType name="CommentType">
        <xs:sequence>
            <xs:element name="CommentType" type="xs:NCName" minOccurs="0"/>
            <xs:element name="CommentText" type="xs:string" minOccurs="0"/>
            <xs:element name="CommentID" type="xs:integer"/>
            <xs:element name="CreatedTimestamp" type="xs:NMTOKEN" minOccurs="0"/>
            <xs:element name="CreatedUserId" type="xs:integer"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="OrderLineItemType">
        <xs:sequence>
            <xs:element name="LineItemSequence" type="xs:integer"/>
            <xs:element name="ItemId" type="xs:string"/>
            <xs:element name="Description" type="xs:string"/>
            <xs:element name="Status" type="xs:NCName"/>
            <xs:sequence minOccurs="0">
                <xs:element name="SuggestedOrderQuantity" type="xs:integer"/>
                <xs:element name="OrderQuantity" type="xs:integer"/>
            </xs:sequence>
            <xs:sequence minOccurs="0">
                <xs:element name="SourceId" type="xs:integer"/>
                <xs:element name="SourceType" type="xs:NCName"/>
            </xs:sequence>
            <xs:element name="LineComment" type="tns:CommentType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="VoidFlag" type="xs:boolean"/>
    </xs:complexType>
    <xs:complexType name="ItemCountType">
        <xs:sequence>
            <xs:element name="ItemID" type="xs:string"/>
            <xs:element name="Quantity" type="xs:decimal"/>
            <xs:element ref="dtv:InventoryBucketId"/>
        </xs:sequence>
        <xs:attribute name="VoidFlag" type="xs:boolean"/>
    </xs:complexType>
    <xs:complexType name="ExchangeType">
        <xs:complexContent>
            <xs:extension base="tns:SaleType">
                <xs:sequence>
                    <xs:element ref="dtv:NewSerialNumber"/>
                    <xs:element ref="dtv:OriginalSerialNumber"/>
                    <xs:element name="Reason" type="xs:string"/>
                    <xs:element ref="dtv:ReasonComment"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="EscrowTransactionType">
        <xs:sequence>
            <xs:element name="TransactionType" type="xs:string"/>
            <xs:element name="Customer" type="tns:PoslogCustomerType"/>
            <xs:element ref="dtv:EscrowTransferToAccount"/>
            <xs:element ref="dtv:EscrowTransferFromAccount"/>
        </xs:sequence>
    </xs:complexType>
    <!-- END Standard POSLog types -->
</xs:schema>
