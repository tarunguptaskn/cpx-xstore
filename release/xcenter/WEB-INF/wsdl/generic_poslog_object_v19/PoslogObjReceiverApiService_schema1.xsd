            <xs:element name="PreviousLayaway" type="tns:PreviousLayawayType" minOccurs="0"/>
            <xs:element name="PreviousCustomerOrder" type="tns:PreviousCustomerOrderType" minOccurs="0"/>
            <xs:element name="CustomerOrderForDelivery" type="tns:CustomerOrderForDeliveryType" minOccurs="0"/>
            <xs:element name="CustomerOrderForPickup" type="tns:CustomerOrderForPickupType" minOccurs="0"/>
            <xs:element name="Return" type="tns:ReturnType" minOccurs="0"/>
            <xs:element name="SaleForDelivery" type="tns:SaleForDeliveryType" minOccurs="0"/>
            <xs:element name="Sale" type="tns:SaleType" minOccurs="0"/>
            <xs:element name="Exchange" type="tns:ExchangeType" minOccurs="0"/>
            <xs:element name="Tender" type="tns:TenderType" minOccurs="0"/>
            <xs:element ref="dtv:Coupon" minOccurs="0"/>
            <xs:element ref="dtv:TransactionDeal" minOccurs="0"/>
            <xs:sequence minOccurs="0">
                <xs:element ref="dtv:DocumentType"/>
                <xs:element ref="dtv:DocumentId"/>
                <xs:element ref="dtv:SeriesId"/>
                <xs:element ref="dtv:ActivityCode"/>
            </xs:sequence>
            <xs:element name="LineNumber" type="xs:integer" minOccurs="0"/>
            <xs:element name="ItemID" type="xs:string" minOccurs="0"/>
            <xs:element ref="dtv:QuantityShipped" minOccurs="0"/>
            <xs:sequence minOccurs="0">
                <xs:element name="SerialNumbers" type="xs:string" minOccurs="0"/>
                <xs:element name="EnteredItemID" type="xs:string" minOccurs="0"/>
            </xs:sequence>
            <xs:element name="TypeCode" type="xs:NCName" minOccurs="0"/>
            <xs:sequence minOccurs="0">
                <xs:element name="ActualCount" type="xs:decimal"/>
                <xs:element name="ExpectedCount" type="xs:decimal"/>
                <xs:element name="PostedCount" type="xs:decimal"/>
                <xs:element name="SaleLineItem" type="tns:SaleLineItemType"/>
            </xs:sequence>
            <xs:element name="RecordCreationType" type="xs:NCName" minOccurs="0"/>
            <xs:element name="StatusCode" type="xs:NCName" minOccurs="0"/>
            <xs:element name="ItemAccountModifier" type="tns:ItemAccountModifierType" minOccurs="0"/>
            <xs:element name="Tax" type="tns:TaxType" minOccurs="0"/>
            <xs:element name="Discount" type="tns:DiscountType" minOccurs="0"/>
            <xs:sequence minOccurs="0">
                <xs:element name="QuantityOrdered" type="xs:decimal"/>
                <xs:element name="QuantityReceived" type="xs:decimal"/>
                <xs:element name="CartonNumber" type="xs:string"/>
            </xs:sequence>
            <xs:sequence minOccurs="0">
                <xs:element name="LineItemNumber" type="xs:integer"/>
                <xs:element name="Description" type="xs:string"/>
            </xs:sequence>
            <xs:element name="PaymentOnAccount" type="tns:PaymentOnAccountType" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="EntryMethod" type="xs:string"/>
        <xs:attribute name="VoidFlag" type="xs:boolean"/>
    </xs:complexType>
    <xs:complexType name="SaleForDeliveryType">
        <xs:complexContent>
            <xs:extension base="tns:SaleType">
                <xs:sequence>
                    <xs:element ref="dtv:OrderType" minOccurs="0"/>
