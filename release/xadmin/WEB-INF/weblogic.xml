<?xml version="1.0" encoding="UTF-8"?>
<weblogic-web-app xmlns="http://www.bea.com/ns/weblogic/weblogic-web-app">
  <context-root>xadmin</context-root>
  <container-descriptor>
    <prefer-application-packages>
      <package-name>com.fasterxml.jackson.*</package-name>
      <package-name>org.jboss.logging.*</package-name>
      <package-name>org.apache.commons.beanutils.*</package-name>
      <package-name>org.apache.commons.codec.*</package-name>
      <package-name>org.apache.commons.collections4.*</package-name>
      <package-name>org.apache.commons.configuration2.*</package-name>
      <package-name>org.apache.commons.csv.*</package-name>
      <package-name>org.apache.commons.fileupload.*</package-name>
      <package-name>org.apache.commons.io.*</package-name>
      <package-name>org.apache.commons.lang3.*</package-name>
      <package-name>org.apache.commons.logging.*</package-name>
      <package-name>org.apache.commons.pool2.*</package-name>
      <package-name>org.apache.commons.text.*</package-name>
      <package-name>org.apache.fontbox.*</package-name>
      <package-name>org.apache.log4j.*</package-name>
      <package-name>org.apache.logging.log4j.*</package-name>
      <package-name>org.apache.logging.slf4j.*</package-name>
      <package-name>org.apache.pdfbox.*</package-name>
      <package-name>org.apache.velocity.*</package-name>
      <package-name>org.springframework.*</package-name>
      <package-name>org.glassfish.jersey.*</package-name>
      <package-name>com.google.common.*</package-name>
      <!-- Added as a workaround until the BIP issue is fixed -->
      <package-name>oracle.xml.*</package-name>
    </prefer-application-packages>
    <prefer-application-resources>
      <resource-name>org.glassfish.jersey.*</resource-name>
      <resource-name>META-INF/services/org.glassfish.jersey.*</resource-name>
      <resource-name>org/springframework/scheduling/config/spring-task.xsd</resource-name>
    </prefer-application-resources>
  </container-descriptor>
</weblogic-web-app>
