         <xs:element name="ExtendedAmount" type="xs:decimal" minOccurs="0"/>
         <xs:element name="Quantity" type="xs:decimal" minOccurs="0"/>
         <xs:element ref="dtv:UnitOfMeasureCode" minOccurs="0"/>
         <xs:element ref="dtv:MeasurementRequired" minOccurs="0"/>
         <xs:element ref="dtv:WeightEntryMethodCode" minOccurs="0"/>
         <xs:element ref="dtv:TareValue" minOccurs="0" />  
