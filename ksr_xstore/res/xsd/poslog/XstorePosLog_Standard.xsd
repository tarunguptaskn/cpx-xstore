      <xs:sequence minOccurs="0">
        <xs:element name="SerialNumbers" type="xs:string" minOccurs="0"/>
        <xs:element name="EnteredItemID" type="xs:string" minOccurs="0"/>
      </xs:sequence>
      <xs:element name="TypeCode" type="xs:NCName" minOccurs="0"/>
      <xs:sequence minOccurs="0">
        <xs:element name="ActualCount" type="xs:decimal"/>
        <xs:element name="ExpectedCount" type="xs:decimal"/>
        <xs:element name="PostedCount" type="xs:decimal"/>
        <xs:element name="SaleLineItem" type="SaleLineItemType"/>
      </xs:sequence>
      <xs:element name="RecordCreationType" type="xs:NCName" minOccurs="0"/>
      <xs:element name="StatusCode" type="xs:NCName" minOccurs="0"/>
      <xs:element name="ItemAccountModifier" type="ItemAccountModifierType" minOccurs="0" />
      <xs:element name="Tax" type="TaxType" minOccurs="0" />
      <xs:element name="Discount" type="DiscountType" minOccurs="0"/>
      <xs:sequence minOccurs="0">
        <xs:element name="QuantityOrdered" type="xs:decimal"/>
        <xs:element name="QuantityReceived" type="xs:decimal"/>
        <xs:element name="CartonNumber" type="xs:string"/>
      </xs:sequence>
      <xs:sequence minOccurs="0">
        <xs:element name="LineItemNumber" type="xs:integer"/>
        <xs:element name="Description" type="xs:string"/>
      </xs:sequence>
      <xs:element name="PaymentOnAccount" type="PaymentOnAccountType" minOccurs="0" />
    </xs:sequence>
    <xs:attribute name="EntryMethod" type="xs:string"/>
    <xs:attribute name="VoidFlag" type="xs:boolean" />
  </xs:complexType>

  <xs:complexType name="SaleForDeliveryType">
    <xs:complexContent>
      <xs:extension base="SaleType">
        <xs:sequence>
          <xs:element ref="dtv:OrderType" minOccurs="0" />
          <xs:element name="AccountNumber" type="xs:string" minOccurs="0" />
          <xs:element name="Delivery" type="DeliveryType" minOccurs="0" />
        </xs:sequence>
        <xs:attribute name="OrderStatus" type="xs:string"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  <xs:complexType name="AssociateType">
    <xs:sequence>
      <xs:element name="AssociateID" type="xs:string" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="RetailPriceModifierType">
    <xs:sequence>
      <xs:element name="SequenceNumber" type="xs:string" />
      <xs:element name="Amount" type="AmountType" />
      <xs:element name="PromotionID" type="xs:string" minOccurs="0"/>
      <xs:element name="ReasonCode" type="xs:string" minOccurs="0"/>
