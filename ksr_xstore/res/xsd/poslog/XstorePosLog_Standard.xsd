      </xs:choice>
      <xs:element name="RetailTransaction" type="RetailTransactionType" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="CancelFlag" type="xs:boolean" />
    <xs:attribute name="TrainingModeFlag" type="xs:boolean" />
    <xs:attribute name="OfflineFlag" type="xs:boolean" />
    <xs:attribute ref="dtv:TransactionType" />
    <xs:attribute ref="dtv:AppVersion" />
    <xs:attribute ref="dtv:crossChannelReturn" />
    <xs:attribute ref="dtv:InventoryDocumentSubType"/>
    <xs:attribute ref="dtv:InventoryDocumentType"/>
  </xs:complexType>
  <xs:complexType name="RetailTransactionType">
    <xs:sequence>
      <xs:element name="LineItem" type="LineItemType" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="Total" type="TotalType" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="RoundedTotal" type="xs:decimal" minOccurs="0" />
      <xs:element name="Customer" type="PoslogCustomerType"  minOccurs="0" />
      <xs:element name="CustomerAccount" type="CustomerAccountType" minOccurs="0" />
      <xs:element ref="dtv:EmployeeSaleId" minOccurs="0" />
      <xs:element name="TransactionLink" type="TransactionLinkType" minOccurs="0" />
      <xs:choice minOccurs="0">
