  <xs:complexType name="LineItemType">
    <xs:sequence>
      <xs:element name="SequenceNumber" type="xs:string" minOccurs="0"/>
      <xs:element name="BeginDateTime" type="xs:dateTime" minOccurs="0" />
      <xs:element name="EndDateTime" type="xs:dateTime" minOccurs="0" />
      <xs:element name="Layaway" type="LayawayType" minOccurs="0" />
      <xs:element name="PreviousLayaway" type="PreviousLayawayType" minOccurs="0" />
      <xs:element name="PreviousCustomerOrder" type="PreviousCustomerOrderType" minOccurs="0" />
      <xs:element name="CustomerOrderForDelivery" type="CustomerOrderForDeliveryType" minOccurs="0" />
      <xs:element name="CustomerOrderForPickup" type="CustomerOrderForPickupType" minOccurs="0" />
      <xs:element name="Return" type="ReturnType" minOccurs="0" />
      <xs:element name="SaleForDelivery" type="SaleForDeliveryType" minOccurs="0"/>
      <xs:element name="Sale" type="SaleType" minOccurs="0" />
      <xs:element name="Exchange" type="ExchangeType" minOccurs="0" />
