         <xs:element name="Tax" type="TaxType" minOccurs="0" maxOccurs="unbounded" />
         <xs:element name="TransactionLink" type="TransactionLinkType" minOccurs="0"/>
         <xs:element ref="dtv:LineItemProperty" maxOccurs="unbounded" minOccurs="0" />
         <xs:element minOccurs="0" maxOccurs="unbounded" ref="dtv:LineItemAssociation"/>
